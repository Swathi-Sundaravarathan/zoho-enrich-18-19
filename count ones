#include<stdio.h>
void main()
{
  int a[100],n,i,count=0,temp=0;
  
  printf("enter the number");
  scanf("%d",&n);
  
  while(n>0)
  {
    a[temp]=n%2;
    n=n/2;
    temp++;
   }
   
   for(i=0;i<temp;i++)
   {
    if(a[i]==1)
      {
      count++;
      }
    }
    
    printf("%d",count);
    
}

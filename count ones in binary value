#include<stdio.h>
void main()
{
  int n,i,temp=0,count=0,p;
  
  printf("enter the number");
  scanf("%d",&n);
  
  while(n>0)
  {
    p=n;
    while(p>1)
    {
      p=p-2;
    }
    a[temp++]=p;
    n=n/2;
  }
  
  for(i=0;i<temp;i++)
  {
    if(a[i]==1)
    { 
      count++;
    }
  }
  
  printf("%d",count);
  
}
